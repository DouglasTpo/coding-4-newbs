<h1>Show#article</h1>

<!--- view para o admin
<h1><%= @article.title %></h1>
<p><%= @article.body %></p>

<% if user_signed_in? && current_user.admin? %>
  <%= link_to 'Edit', edit_article_path(@article) %> |
  <%= link_to 'Delete', @article, method: :delete, data: { confirm: 'Are you sure?' } %>
<% end %>
--->
